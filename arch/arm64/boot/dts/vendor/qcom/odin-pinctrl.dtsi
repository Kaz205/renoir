/*Display Reset, DispRate, Err_Int Pin*/
&sde_dsi_active {
	mux {
		pins = "gpio24", "gpio42", "gpio75", "gpio83";
		function = "gpio";
	};

	config {
		pins = "gpio24", "gpio42", "gpio75", "gpio83";
		drive-strength = <8>;	/* 8 mA */
		bias-disable = <0>;   /* no pull */
	};
};

&sde_dsi_suspend {
	mux {
		pins = "gpio24", "gpio30", "gpio42", "gpio75", "gpio83";
		function = "gpio";
	};

	config {
		pins = "gpio24", "gpio30", "gpio42", "gpio75", "gpio83";
		drive-strength = <2>;	/* 2 mA */
		bias-pull-down; 	/* PULL DOWN */
	};
};

